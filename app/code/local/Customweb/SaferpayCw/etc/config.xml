	<adminhtml>
		<layout>
			<updates>
				<saferpaycw>
					<file>saferpaycw.xml</file>
				</saferpaycw>
			</updates>
		</layout>
		<translate>
			<modules>
				<Customweb_SaferpayCw>
					<files>
						<default>Customweb_SaferpayCw.csv</default>
					</files>
				</Customweb_SaferpayCw>
			</modules>
		</translate>
	</adminhtml>
	
	<crontab>
		<jobs>
			<saferpaycw_cron>
				<schedule>
					<cron_expr>*/5 * * * *</cron_expr>
				</schedule>
				<run>
					<model>saferpaycw/CronObserver::executeCron</model>
				</run>
			</saferpaycw_cron>
		</jobs>
	</crontab>

	<admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <Customweb_SaferpayCw after="Mage_Adminhtml">Customweb_SaferpayCw</Customweb_SaferpayCw>
                    </modules>
                </args>
            </adminhtml>
        </routers>
