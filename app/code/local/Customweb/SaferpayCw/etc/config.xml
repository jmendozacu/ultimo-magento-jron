<?xml version="1.0" encoding="UTF-8"?>
 <config>
	<modules>
		<Customweb_SaferpayCw>
			<version>1.4.0</version>
		</Customweb_SaferpayCw>
	</modules>
	<global>
		<models>
			<saferpaycw>
				<class>Customweb_SaferpayCw_Model</class>
				<resourceModel>saferpaycw_mysql4</resourceModel>
			</saferpaycw>
			<saferpaycw_mysql4>
				<class>Customweb_SaferpayCw_Model_Mysql4</class>
				<entities>
					<aliasdata>
						<table>saferpaycw_alias_data</table>
					</aliasdata>
					<customercontext>
						<table>saferpaycw_customer_context</table>
					</customercontext>
					<transaction>
						<table>saferpaycw_transaction</table>
					</transaction>
					<storage>
						<table>saferpaycw_storage</table>
					</storage>
					<externalcheckoutcontext>
						<table>saferpaycw_external_checkout_context</table>
					</externalcheckoutcontext>
				</entities>
			</saferpaycw_mysql4>
			
			<checkout>
				<rewrite>
					<cart>Customweb_SaferpayCw_Model_Checkout_Cart</cart>
				</rewrite>
			</checkout>
		</models>
		<blocks>
			<saferpaycw><class>Customweb_SaferpayCw_Block</class></saferpaycw>
			
			<adminhtml>
				<rewrite>
					<sales_order_invoice_view>Customweb_SaferpayCw_Block_Adminhtml_Sales_Order_Invoice_View</sales_order_invoice_view>
				</rewrite>
			</adminhtml>
		</blocks>
		<helpers>
			<SaferpayCw>
				<class>Customweb_SaferpayCw_Helper</class>
			</SaferpayCw>
		</helpers>
		<resources>
			<saferpaycw_write>
				<connection>
					<use>core_write</use>
				</connection>
			</saferpaycw_write>
			<saferpaycw_read>
				<connection>
					<use>core_read</use>
				</connection>
			</saferpaycw_read>
			<saferpaycw_setup>
				 <setup>
				 	<module>Customweb_SaferpayCw</module>
				 	<class>Customweb_SaferpayCw_Model_Resource_Setup</class>
				 </setup>
			</saferpaycw_setup>
		</resources>
		<events>
			<adminhtml_sales_order_invoice_view_construct>
				<observers>
					<saferpaycw_invoice_view>
						<type>singleton</type>
						<class>Customweb_SaferpayCw_Model_Observer</class>
						<method>invoiceView</method>
					</saferpaycw_invoice_view>
				</observers>
			</adminhtml_sales_order_invoice_view_construct>
			<checkout_cart_init>
				<observers>
					<saferpaycw_init_cart>
						<type>singleton</type>
						<class>Customweb_SaferpayCw_Model_Observer</class>
						<method>initCart</method>
					</saferpaycw_init_cart>
				</observers>
			</checkout_cart_init>
			<sales_order_place_after>
				<observers>
					<saferpaycw_place_order>
						<type>singleton</type>
						<class>Customweb_SaferpayCw_Model_Observer</class>
						<method>placeOrder</method>
					</saferpaycw_place_order>
				</observers>
			</sales_order_place_after>
			<sales_order_save_after>
				<observers>
					<saferpaycw_save_order>
						<type>singleton</type>
						<class>Customweb_SaferpayCw_Model_Observer</class>
						<method>saveOrder</method>
					</saferpaycw_save_order>
				</observers>
			</sales_order_save_after>
			<sales_order_payment_capture>
				<observers>
					<saferpaycw_capture_payment>
						<type>singleton</type>
						<class>Customweb_SaferpayCw_Model_Observer</class>
						<method>capturePayment</method>
					</saferpaycw_capture_payment>
				</observers>
			</sales_order_payment_capture>
			<order_cancel_after>
				<observers>
					<saferpaycw_cancel_order>
						<type>singleton</type>
						<class>Customweb_SaferpayCw_Model_Observer</class>
						<method>cancelOrder</method>
					</saferpaycw_cancel_order>
				</observers>
			</order_cancel_after>
			<controller_action_postdispatch_adminhtml_sales_order_create_save>
				<observers>
					<saferpaycw_moto_after_save>
						<type>singleton</type>
						<class>Customweb_SaferpayCw_Model_MotoObserver</class>
						<method>postOrderSave</method>
					</saferpaycw_moto_after_save>
				</observers>
			</controller_action_postdispatch_adminhtml_sales_order_create_save>
			<controller_action_predispatch_adminhtml_sales_order_create_save>
				<observers>
					<saferpaycw_moto_before_save>
						<type>singleton</type>
						<class>Customweb_SaferpayCw_Model_MotoObserver</class>
						<method>beforeOrderSave</method>
					</saferpaycw_moto_before_save>
				</observers>
			</controller_action_predispatch_adminhtml_sales_order_create_save>
			<load_customer_quote_before>
				<observers>
					<saferpaycw_load_customer_quote_before>
						<type>singleton</type>
						<class>Customweb_SaferpayCw_Model_Observer</class>
						<method>loadCustomerQuoteBefore</method>
					</saferpaycw_load_customer_quote_before>
				</observers>
			</load_customer_quote_before>
			<customweb_externalcheckout_widgets_collect>
				<observers>
					<saferpaycw_externalcheckout_widgets_collect>
						<type>singleton</type>
						<class>Customweb_SaferpayCw_Model_Observer</class>
						<method>collectExternalCheckoutWidgets</method>
					</saferpaycw_externalcheckout_widgets_collect>
				</observers>
			</customweb_externalcheckout_widgets_collect>
			
			<controller_front_init_before>
				<observers>
					<saferpaycw_register_translation_resolver>
						<type>singleton</type>
						<class>Customweb_SaferpayCw_Model_Observer</class>
						<method>registerTranslationResolver</method>
					</saferpaycw_register_translation_resolver>
				</observers>
			</controller_front_init_before>
		</events>
		<sales>
			<order>
				<statuses>
					<canceled_saferpaycw translate="label">
						<label><![CDATA[Canceled SaferpayCw]]></label>
					</canceled_saferpaycw>
					<pending_saferpaycw translate="label">
						<label><![CDATA[Pending SaferpayCw]]></label>
					</pending_saferpaycw>
				</statuses>
				<states>
					<canceled>
						<statuses>
							<canceled_saferpaycw />
						</statuses>
					</canceled>
					<pending_payment>
						<statuses>
							<pending_saferpaycw />
						</statuses>
					</pending_payment>
				</states>
			</order>
		</sales>
